{
  "id": "entity-export",
  "name": "Entity Export",
  "description": "Combine all .md files from active entity into a single export file",
  "verb": "execute",
  "scope": "global",
  "requires_dry_run": false,
  "dry_run_command": null,
  "command": "powershell -ExecutionPolicy Bypass -File \"{BOND_ROOT}/panel/powershell/scripts/entity_export.ps1\" -EntityPath \"{ENTITY_PATH}\" -BondRoot \"{BOND_ROOT}\"",
  "params": [
    { "name": "ENTITY_PATH", "source": "active_entity", "validate": "doctrine_path" }
  ],
  "post_verify": null
}
